{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*.astro"
      ],
      "@/layouts/*": [
        "src/layouts/*.astro"
      ],
      "@/util": [
        "src/util.ts"
      ],
      "@/stores/*": [
        "src/stores/*"
      ],
      "@/types": [
        "src/types.ts"
      ]
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": [
      "@astrojs/image/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}